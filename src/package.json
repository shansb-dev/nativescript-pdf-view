{
  "name": "nativescript-pdf-view",
  "version": "0.0.0-development",
  "description": "A NativeScript plugin to display PDF files on iOS and Android",
  "main": "pdf-view",
  "typings": "index.d.ts",
  "nativescript": {
    "platforms": {
      "ios": "6.0.0",
      "android": "6.0.0"
    }
  },
  "scripts": {
    "build": "tsc",
    "commit": "git-cz",
    "commitmsg": "cz-customizable-ghooks",
    "demo.ios": "npm run preparedemo && cd ../demo && tns run ios --emulator",
    "demo.ios.device": "npm run preparedemo && cd ../demo && tns run ios",
    "demo.android": "npm run preparedemo && cd ../demo && tns run android --emulator",
    "lint": "tslint --project tsconfig.json -t stylish",
    "test.ios": "cd ../demo && tns test ios --emulator",
    "test.ios.device": "cd ../demo && tns test ios",
    "test.android": "cd ../demo && tns test android",
    "preparedemo": "npm run build && cd ../demo && tns plugin remove nativescript-pdf-view && tns plugin add ../src && tns install",
    "prepare": "npm run build",
    "semantic-release": "semantic-release",
    "setup": "npm ci && cd demo && npm ci && cd .. && npm run build && cd demo && tns plugin add .. && cd ..",
    "postinstall": "patch-package && ts-patch install",
    "ng-packagr": "ng-packagr -p angular/package.json -c angular/tsconfig.angular.json",
    "clean-setup-demo": "cd ../demo && ns clean && npm install",
    "clean-setup": "ns clean && npm install && npm run ng-packagr"
  },
  "release": {
    "generateNotes": "./node_modules/@semantic-release/release-notes-generator"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/madmas/nativescript-pdf-view.git"
  },
  "keywords": [
    "Android",
    "JavaScript",
    "NativeScript",
    "PDF",
    "View",
    "iOS"
  ],
  "contributors": [
    {
      "name": "Merott Movahedi",
      "email": "merott@merott.com"
    },
    {
      "name": "Markus Schlichting",
      "email": "m.schlichting@mynethome.de"
    }
  ],
  "bugs": {
    "url": "https://github.com/madmas/nativescript-pdf-view/issues"
  },
  "license": "MIT",
  "homepage": "https://github.com/madmas/nativescript-pdf-view",
  "readmeFilename": "README.md",
  "devDependencies": {
    "@angular/animations": "^10.1.3",
    "@angular/common": "^10.1.3",
    "@angular/core": "^10.1.3",
    "@angular/platform-browser": "^10.1.3",
    "@angular/platform-browser-dynamic": "^10.1.3",
    "@angular/router": "^10.1.3",
    "@nativescript/angular": "~10.1.0",
    "@angular/forms": "^10.1.3",
    "@angular-devkit/core": "^10.1.3",
    "@angular/cli": "^10.1.3",
    "@angular/compiler": "^10.1.3",
    "@angular/compiler-cli": "^10.1.3",
    "@ngtools/webpack": "^10.1.3",
    "@nativescript/core": "^7.0.8",
    "@nativescript/plugin-tools": "~1.0.0",
    "@nativescript/types": "~7.0.0",
    "@nativescript/webpack": "^3.0.5",
    "ng-packagr": "^10.1.2",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "semantic-release": "^17.1.1",
    "prompt": "~1.0.0",
    "rimraf": "3.0.0",
    "ts-patch": "^1.3.0",
    "patch-package": "^6.2.2",
    "tslint": "^6.1.3",
    "tslib": "^2.0.1",
    "typescript": "~3.9.7",
    "zone.js": "~0.10.3"
  }
}
